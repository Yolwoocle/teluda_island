version 33
__lua__
--teluda's island
--janshariff, yolwoocle, azure48, m13
function _init()
 setfont()
 game = {state = "main"}
 music(0)
	init_player()
end

function _update()
 if game.state == "main" then
	 upd_player()
  -- start a battle, debug only
  if(btn(5))init_battle()
 elseif game.state == "battle" then
  upd_battle()
 end
end

function _draw()
 if game.state == "main" then
  draw_main()
 elseif game.state == "battle" then
  draw_battle()
 end
end

function draw_main()
 --change camera position depending on what room teluda is in
 camera((p.x + 4)\128*128, (p.y + 8)\128*128)
 --clear the screen with green to fill any gaps
 --this is probably temporary
 cls(11)
 --draw the map & teluda
 map()
 drw_player()
 --draw the tiles that overlap teluda
 map(0,0,0,0,128,64,0x80)
end

-- print in sitelen pona
function pprint(str,x,y,c) print("\014" .. str,x,y,c or peek(0x5f25)) end

